{
  "name": "snippets",
  "version": "1.0.0",
  "description": "Insert predefined snippets",
  "scripts": {
    "prepublishOnly": "pnpm lint && pnpm build",
    "lint": "eslint src/**/*.ts",
    "build": "node build.js && pnpm build-copy",
    "build-copy": "mkdir -p ../../assets/scripts/tinymce/plugins/ && cp -r dist/snippets/ ../../assets/scripts/tinymce/plugins/snippets/ && mkdir -p ../../assets/vendor/tinymce/plugins/ && cp -r dist/snippets/ ../../assets/vendor/tinymce/plugins/snippets/",
    "test": "bedrock-auto -b chrome-headless -d src/test/ts/",
    "test-manual": "bedrock -d src/test/ts/"
  },
  "keywords": [],
  "author": "FriendsOfREDAXO",
  "devDependencies": {
    "@ephox/agar": "latest",
    "@ephox/bedrock-server": "latest",
    "@ephox/mcagar": "latest",
    "@types/mocha": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "chai": "^4.2.0",
    "esbuild": "^0.18.0",
    "eslint": "^7.0.0",
    "tinymce": "^8.2.2",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "files": [
    "lib/main",
    "lib/demo",
    "lib/test",
    "src",
    "tsconfig.json",
    "readme.md",
    "LEGAL.txt",
    "CHANGELOG.txt",
    "LICENSE.txt"
  ],
  "main": "./lib/main/ts/api/Main.js",
  "module": "./lib/main/ts/api/Main.js",
  "types": "./lib/main/ts/api/Main.d.ts",
  "license": "MIT"
}
